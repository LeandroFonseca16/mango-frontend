{
  "name": "mangobeat-nextjs",
  "version": "2.0.0",
  "private": true,
  "description": "MangoBeat AI - Next.js Full-Stack Music Generation Platform",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
  "trends:generate-samples": "tsx scripts/trends/generate-sample-audio.ts",
    "trends:ingest": "tsx scripts/trends/ingest-trends.ts",
    "trends:features": "tsx scripts/trends/extract-features.ts",
    "trends:refresh": "npm run trends:ingest && npm run trends:features"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@prisma/client": "^6.19.0",
    "@tanstack/react-query": "^5.59.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
  "fs-extra": "^11.2.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.553.0",
    "nanoid": "^5.0.9",
    "next": "^15.0.0",
    "openai": "^4.76.0",
  "p-limit": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "zustand": "^4.5.2",
    "recharts": "^3.3.0",
    "resend": "^4.0.1",
    "sharp": "^0.33.5",
    "socket.io-client": "^4.8.1",
    "stripe": "^17.5.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
  "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8.4.49",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
